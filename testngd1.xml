    <classes>
       <class name="Ovi_Test.Demo1Test">
       <methods>
         <include name="Demo"/>
       </methods>
       </class>
       <class name="Ovi_Test.Demo2Test"/>
       <class name="Ovi_Test.Demo4Test"/>
