   </test> 

    <test name="Car Loan">
	<classes>
		 <class name="Ovi_Test.Demo3Test">
		 <methods>
		   <exclude name="Mobile.*"/>
		 </methods>
		 </class>
	</classes>
    </test> 
  
</suite>

